{
	"name": "v1r's Custom Scrollbar",
	"description": "Gives you the ability to easily customize your scrollbar.",
	"author": "emy v1r Sixem LLC",
	"manifest_version": 2,
	"version": "1.2.2",
	"homepage_url": "https://v1r.eu/",
	"icons": {
		"128": "128.png"
	},
	"browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
	"background":
	{
		"scripts": ["main.js"],
		"persistent" : false,
		"run_at": "document_start"
	},
	"permissions": ["activeTab", "storage", "<all_urls>"]
}
